# Prefix: Languagesystems
# automatic
languagesystem DFLT dflt;
languagesystem grek dflt;
languagesystem latn dflt;
languagesystem latn CAT;
languagesystem latn TRK;

# automatic
@Uppercase = [ A Agrave Aacute Acircumflex Atilde Adieresis Aring Amacron Abreve Aogonek uni01CD uni0200 uni0202 uni0226 B uni1E02 C Ccedilla Cacute Ccircumflex Cdotaccent Ccaron D Dcaron uni1E0A E Egrave Eacute Ecircumflex Edieresis Emacron Ebreve Edotaccent Eogonek Ecaron uni0204 uni0206 uni0228 uni1EBC F uni1E1E G Gcircumflex Gbreve Gdotaccent Gcaron uni01F4 H Hcircumflex uni021E I Igrave Iacute Icircumflex Idieresis Itilde Imacron Ibreve Iogonek Idotaccent uni01CF uni0208 uni020A J Jcircumflex K uni01E8 L Lacute Lcaron M uni1E40 N Ntilde Nacute Ncaron uni01F8 O Ograve Oacute Ocircumflex Otilde Odieresis Omacron Obreve Ohungarumlaut uni01D1 uni01EA uni020C uni020E uni022E P uni1E56 Q R Racute Rcaron uni0210 uni0212 S Sacute Scircumflex Scedilla Scaron uni1E60 T Tcaron uni021A uni1E6A U Ugrave Uacute Ucircumflex Udieresis Utilde Umacron Ubreve Uring Uhungarumlaut Uogonek uni01D3 uni0214 uni0216 V W Wcircumflex Wgrave Wacute Wdieresis X Y Yacute Ycircumflex Ydieresis uni0232 Ygrave uni1EF8 Z Zacute Zdotaccent Zcaron AE uni01E2 AEacute Eth Oslash Oslashacute Thorn Dcroat Hbar IJ Ldot Lslash Eng OE Tbar uni01C4 uni01C7 uni01CA uni01F1 Delta uni01C5 uni01C8 uni01CB uni01F2 Aringacute uni0162 uni0394 Sigma Omega uni03A9 uni0122 uni0136 uni013B uni0145 uni0156 uni0218 ];

feature aalt {
# automatic
feature locl;
feature frac;
feature ordn;
} aalt;

feature locl {
# automatic
script latn;
language CRT;
sub i by idotaccent;
language TAT;
sub i by idotaccent;
language KAZ;
sub i by idotaccent;
language CAT;
sub  l' periodcentered' l by ldot;
sub  L' periodcentered' L by Ldot;
language AZE;
sub i by idotaccent;
language TRK;
sub i by idotaccent;
} locl;

feature frac {
# automatic
sub one slash four by onequarter;
sub one slash two by onehalf;
sub three slash four by threequarters;
} frac;

feature ordn {
# automatic
sub [zero one two three four five six seven eight nine] [A a]' by ordfeminine;
sub [zero one two three four five six seven eight nine] [O o]' by ordmasculine;
} ordn;

feature dlig {
# automatic
sub f f by uniFB00;
sub f f i by uniFB03;
sub f f l by uniFB04;
sub f i by uniFB01;
sub f l by uniFB02;
} dlig;

feature cpsp {
# automatic
pos @Uppercase <5 0 10 0>;
} cpsp;

